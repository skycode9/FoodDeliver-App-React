{
  "rewrites": [
    {
      "source": "/api/dapi/(.*)",
      "destination": "https://www.swiggy.com/dapi/$1"
    }
  ],
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
